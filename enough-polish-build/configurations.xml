<configurations>
	
	<configuration>
		<identifier>CLDC/1.0</identifier>
		<features>cldc, cldc1.0</features>
		<description>Connected Limited Device Configuration 1.0. Does not support floating point calculations.</description>
		<capability name="build.BootClassPath" value="cldc-1.0.jar" />
		<capability name="SupportFloatingPoint" value="false" />
	</configuration>

